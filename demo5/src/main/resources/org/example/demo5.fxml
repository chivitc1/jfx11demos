<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Ellipse?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Reflection?>
<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="350.0" prefWidth="350.0" spacing="50" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Demo5Controller">
   <children>
      <StackPane fx:id="stackPane" onMouseClicked="#handleMouseClick" prefHeight="150.0" prefWidth="200.0">
         <children>
            <Ellipse fx:id="ellipse" radiusX="110.0" radiusY="80.0" stroke="BLACK" strokeType="INSIDE">
               <fill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="BLACK" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
                <effect>
                    <DropShadow color="GREY" offsetX="5.0" offsetY="5.0"/>
                </effect>
            </Ellipse>
            <Text fx:id="text1" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello World">
               <font>
                  <Font size="24.0" />
               </font>
                <effect>
                    <Reflection fraction="0.8" topOffset="1.0"/>
                </effect>
            </Text>
         </children>
      </StackPane>
      <Text fx:id="text2" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
         <font>
            <Font name="FreeSans" size="18.0" />
         </font>
      </Text>
   </children>
</VBox>
